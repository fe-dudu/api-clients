{
  "name": "sync-api-kit",
  "version": "1.0.0",
  "author": "fe-dudu <ehehwhdwhd@naver.com>",
  "description": "Swagger-based clients for multiple APIs for the frontend, with daily type checks.",
  "keywords": ["openapi-typescript", "daily type check"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fe-dudu/sync-api-kit.git"
  },
  "files": ["dist"],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "rimraf": "rimraf",
    "check": "biome check --write --unsafe",
    "generate-type": "openapi-typescript --properties-required-by-default true --alphabetize true && pnpm check",
    "changeset": "changeset",
    "changeset:publish": "pnpm build && changeset publish",
    "changeset:version": "changeset version && pnpm i --lockfile-only"
  },
  "dependencies": {
    "openapi-fetch": "^0.13.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.12",
    "openapi-typescript": "^7.6.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "packageManager": "pnpm@9.14.4",
  "publishConfig": {
    "access": "public"
  }
}
